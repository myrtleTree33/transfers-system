    "rehype-slug": "^6.0.0",
    "svelte-marquee-text-widget": "^1.1.0",
    "sveltekit-superforms": "^2.12.4",
    "tailwind-merge": "^2.2.2",
    "tailwindcss-animated": "^1.0.1",
    "typewriter-effect": "^2.21.0",
    "zod": "^3.22.4"
  }
